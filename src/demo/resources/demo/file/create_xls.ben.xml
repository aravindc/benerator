<?xml version="1.0" encoding="iso-8859-1"?>
<setup	xmlns="http://databene.org/benerator/0.5.6"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://databene.org/benerator/0.5.6 http://databene.org/benerator-0.5.6.xsd">
    <create-entities name="transaction" count="100">
        <variable name="product" type="entity" source="demo/file/products.import.csv" distribution="random"/> <!--cyclic="true"
                  proxy="skip" proxy-param1="1" proxy-param2="2"/-->
        <attribute name="comment" pattern="[A-Z][a-z]{5,12}"/>
        <attribute name="id" type="long" min="1" max="1000000000" distribution="step"/>
        <attribute name="ean_code" source="product.ean_code"/>
        <attribute name="price" source="product.price"/>
        <attribute name="items" type="short" min="1" max="27"/>
        <consumer class="org.databene.platform.xls.XLSEntityExporter">
            <property name="uri" value="transactions.xls"/>
            <property name="properties" value="id,ean_code,comment,price,items"/>
        </consumer>
    </create-entities>
</setup>