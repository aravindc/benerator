<?xml version="1.0" encoding="iso-8859-1"?>
<!--!DOCTYPE setup SYSTEM "http://databene.org/benerator-0.3.dtd"-->
<setup>
    <create-entities name="transaction" count="100">
        <variable name="product" source="shop/products.import.csv" distribution="random"/> <!--cyclic="true"
                  proxy="skip" proxy-param1="1" proxy-param2="2"/-->
        <attribute name="id" type="long" min="1" max="1000000000" distribution="step"/>
        <attribute name="ean_code" source="product.ean_code"/>
        <attribute name="price" source="product.price"/>
        <attribute name="items" type="short" min="1" max="27"/>
        <processor class="org.databene.platform.flat.FlatFileEntityExporter">
            <property name="uri" value="transactions.flat"/>
            <property name="properties" value="id[8r0],ean_code[13],price[8r0],items[4r0]"/>
        </processor>
    </create-entities>
</setup>