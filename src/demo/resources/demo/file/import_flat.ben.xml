<?xml version="1.0" encoding="iso-8859-1"?>
<setup	xmlns="http://databene.org/benerator-0.5.5.xsd"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://databene.org/benerator-0.5.5.xsd org/databene/benerator/benerator-0.5.5.xsd">
		
    <bean id="products_flat" class="org.databene.platform.flat.FlatFileEntityIterable">
        <property name="uri" value="demo/file/products.import.flat"/>
        <property name="entity" value="product"/>
        <property name="properties" value="ean_code[13],name[30],category_id[9],price[8r0],manufacturer[30]"/>
    </bean>

    <create-entities name="product" source="products_flat">
        <consumer class="org.databene.model.consumer.ConsoleExporter"/>
    </create-entities>
</setup>