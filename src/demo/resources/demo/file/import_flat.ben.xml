<?xml version="1.0" encoding="iso-8859-1"?>
<setup	xmlns="http://databene.org/benerator/0.5.7"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://databene.org/benerator/0.5.7 http://databene.org/benerator-0.5.7.xsd">
		
	<import defaults="true"/>
	
    <bean id="products_flat" class="FlatFileEntitySource">
        <property name="uri" value="demo/file/products.import.flat"/>
        <property name="entity" value="product"/>
        <property name="properties" value="ean_code[13],name[30],category_id[9],price[8r0],manufacturer[30]"/>
    </bean>

    <create-entities name="product" source="products_flat" consumer="ConsoleExporter"/>
</setup>