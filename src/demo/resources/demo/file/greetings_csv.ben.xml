<?xml version="1.0" encoding="iso-8859-1"?>
<setup	xmlns="http://databene.org/benerator/0.5.8"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://databene.org/benerator/0.5.8 http://databene.org/benerator-0.5.8.xsd">
    <create-entities name="greeting" count="10">
        <attribute name="salutation" values="Hello,Hi,Howdy"/>
        <attribute name="name" values="Alice,Bob,Charly"/>
        <consumer id="csv" class="org.databene.platform.csv.CSVEntityExporter">
            <property name="uri"     value="greetings.csv"/>
            <property name="columns" value="salutation,name"/>
        </consumer>
    </create-entities>
</setup>