<?xml version="1.0" encoding="UTF-8"?>
<setup 	xmlns="http://databene.org/benerator/0.5.9"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://databene.org/benerator/0.5.9 http://databene.org/benerator-0.5.9.xsd"
        defaultEncoding="${setup.encoding}"
        defaultLineSeparator="${setup.lineSeparator}"
        defaultLocale="${setup.locale}"
		defaultDataset="${setup.dataset}">

    <import defaults="true"/>
    
    <import domain="person"/>
    <import domain="net"/>
    <import domain="product"/>

    <include uri="${setup.projectName}.properties" />
    
    <echo message="Starting ${setup.projectName} data generation" />

    <comment>define the database using the properties set in pom.xml</comment>
    <database id="db" url="{${dbUrl}}" driver="{${dbDriver}}" 
        schema="{${dbSchema}}" user="{${dbUser}}" password="{${dbPassword}}" />

    <comment>execute initializers and load static data</comment>
    ${import-files}
	
	<comment>generate mass data</comment>
	${create-entities}
	
</setup>